                            "altText": "",
                            "sizes": [
                                {
                                    "name": "polox-hernandez-itGcCnMWPpQ-unsplash__1606859155546__w1200.jpg",
                                    "url": "https://www.arcgis.com/sharing/rest/content/items/536c080d27194e38931fc638612c1f68/resources/polox-hernandez-itGcCnMWPpQ-unsplash__1606859155546__w1200.jpg",
                                    "width": 1200
                                }
                            ],
                            "display": "fill"
                        }
